CREATE OR REPLACE TRIGGER BOSNDS3.TIVIPINTEGRALFTP 
    BEFORE INSERT ON BOSNDS3.FA_VIPINTEGRAL_FTP 
    FOR EACH ROW 
declare

  v_id             number(10);
  v_integral_valid c_viptype.integral_valid%type;
  v_integral_month c_viptype.integral_month%type;
  v_integral_day   c_viptype.integral_day%type;
  v_monthday       varchar2(4);
  v_year           varchar2(4);
  v_clearday       varchar2(8);
  v_clearmonthday  varchar2(16); --清除日期
  v_count          number;
  v_integral       number;
  v_isvouchers     varchar2(2) := 'N';

begin
  --begin add by liliangyong 20130425 由于insert的地方太多，所以在这里统一增加结算日期
  select b.integral_valid, b.integral_month, b.integral_day
    into v_integral_valid, v_integral_month, v_integral_day --获取有效期，清除月、日
    from c_vip a, c_viptype b
   where a.c_viptype_id = b.id
     and a.id = :new.c_vip_id;

  --获取系统年月
  select to_char(add_months(to_date(:new.changdate, 'YYYYMMDD'),
                             v_integral_valid),
                  'MMDD'),
         to_char(add_months(to_date(:new.changdate, 'YYYYMMDD'),
                             v_integral_valid),
                  'YYYY')
    into v_monthday, v_year
    from dual;

  select trim(to_char(v_integral_month, '00')) ||
          trim(to_char(v_integral_day, '00'))
    into v_clearday --获取清除日
    from dual;

  --计算清除日期
  if v_monthday <= v_clearday then
    v_clearmonthday := v_year || v_clearday;
  else
    v_clearmonthday := v_year + 1 || v_clearday;
  end if;

  --更新单据表
  if :new.description not like '%充值%' then
    :new.integral_clear := v_clearmonthday;
  end if;
  --end add by liliangyong 20130425

  --判断零售单店仓是否开启积分转券功能
  if :new.description like '由零售单%' then
  
    select sto.is_vouchers
      into v_isvouchers
      from c_store sto
     where sto.id =
           (select c_store_id from m_retail a where a.docno = :new.docno);
  end if;

  select max(id)
    into v_id
    from fa_vipacc
   where ad_client_id = :new.ad_client_id
     and c_vip_id = :new.c_vip_id
     and rownum < 2;

  if v_id is null then
    v_id := get_sequences('FA_VIPACC');
    -- insert FA_VIPACC
  
    insert into fa_vipacc
      (id, ad_client_id, ad_org_id, ownerid, modifierid, creationdate,
       modifieddate, isactive, c_vip_id, amount, integral)
    values
      (v_id, :new.ad_client_id, :new.ad_org_id, :new.ownerid, :new.ownerid,
       sysdate, sysdate, 'Y', :new.c_vip_id, :new.vip_payamt, 0);
  
  else
  
    --update
    update fa_vipacc
       set amount = nvl(amount, 0) + :new.vip_payamt
     where id = v_id;
  
  end if;

  if v_isvouchers = 'N' and :new.description not like '由积分换券%' then
    v_integral := :new.integral;
  
    if v_integral > 0 then
      select count(1)
        into v_count
        from c_client_vip a, c_viptype b
       where a.c_viptype_id = b.id
         and b.if_freeze = 'Y'
         and b.freeze_day > 0
         and :new.docno like 'RE%'
         and a.id = :new.c_vip_id;
    
      if v_count > 0 then
        :new.freeze_integral := v_integral;
        update fa_vipacc t
           set t.freeze_integral = nvl(t.freeze_integral, 0) + v_integral
         where t.c_vip_id = :new.c_vip_id;
      
      else
        update fa_vipacc t
           set t.integral = nvl(t.integral, 0) + v_integral
         where t.c_vip_id = :new.c_vip_id;
      end if;
    
    end if;
  
    if v_integral < 0 then
      select a.freeze_integral
        into v_count
        from fa_vipacc a
       where a.c_vip_id = :new.c_vip_id;
    
      if v_count > 0 and :new.docno like 'RE%' then
      
        update fa_vipacc t
           set t.freeze_integral = t.freeze_integral -
                                   least(t.freeze_integral, abs(v_integral))
         where t.c_vip_id = :new.c_vip_id;
      
        v_integral := v_integral + least(v_count, abs(v_integral));
      end if;
      update fa_vipacc t
         set t.integral = nvl(t.integral, 0) + v_integral
       where t.c_vip_id = :new.c_vip_id;
    end if;
  end if;
end; 
/

