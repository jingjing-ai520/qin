CREATE OR REPLACE TRIGGER BOSNDS3.TIFASUPPLIERFTP 
    BEFORE INSERT ON BOSNDS3.FA_SUPPLIER_FTP 
    FOR EACH ROW 
DECLARE
    v_id NUMBER(10); --供应商资金账户
BEGIN
    --获取供应商资金账户
    SELECT MAX(id)
    INTO v_id
    FROM fa_supplier
    WHERE ad_client_id = :NEW.ad_client_id
    AND c_supplier_id = :NEW.c_supplier_id
    AND rownum < 2;

    IF v_id IS NULL THEN
        --如果资金账户不存，则插入一条供应商的资金账户、应付款、已付金额
        --add xkk 20160415 增加字段[应付押金、已付押金]赋值
        INSERT INTO fa_supplier
            (id, ad_client_id, ad_org_id, ownerid, modifierid, creationdate,
             modifieddate, isactive, c_supplier_id, amt_payable, amt_pay,
             deposit_payable, deposit_pay)
        VALUES
            (get_sequences('FA_SUPPLIER'), :NEW.ad_client_id, :NEW.ad_org_id,
             :NEW.ownerid, :NEW.ownerid, SYSDATE, SYSDATE, 'Y',
             :NEW.c_supplier_id, :NEW.amt_payable, :NEW.amt_pay,
             :NEW.deposit_payable, :NEW.deposit_pay);
    ELSE
        --如果资金账户不存，则更新 应付款=应付款+本次应付款、已付金额=已付金额+本次已付金额
        UPDATE fa_supplier
        SET amt_payable = amt_payable + :NEW.amt_payable,
            amt_pay = amt_pay + :NEW.amt_pay,
            deposit_payable = nvl(deposit_payable, 0) + :NEW.deposit_payable,
            deposit_pay = nvl(deposit_pay, 0) + :NEW.deposit_pay
        WHERE id = v_id;

    END IF;

    --add by csy 20121213 更新供应商资金账里资金余额=已付金额-应付款
    UPDATE c_supplier b
    SET b.feeremain = (SELECT a.amt_pay - a.amt_payable
                        FROM fa_supplier a
                        WHERE nvl(b.feeremain, 0) <> a.amt_pay - a.amt_payable
                        AND a.c_supplier_id = b.id)
    WHERE EXISTS (SELECT 1
           FROM fa_supplier a
           WHERE nvl(b.feeremain, 0) <> a.amt_pay - a.amt_payable
           AND a.c_supplier_id = b.id)
    AND b.id = :NEW.c_supplier_id;
    --end by csy 20121213

    --修改[供应商档案]的字段[押金余额  DEPOSITREMAIN]=[已付押金-应付押金]。
    --add by xkk  20160415 更新供应商押金余额
    UPDATE c_supplier b
    SET b.depositremain = (SELECT nvl(a.deposit_pay, 0) -
                                    nvl(a.deposit_payable, 0)
                            FROM fa_supplier a
                            WHERE nvl(b.depositremain, 0) <>
                                  nvl(a.deposit_pay, 0) -
                                  nvl(a.deposit_payable, 0)
                            AND a.c_supplier_id = b.id)
    WHERE EXISTS (SELECT 1
           FROM fa_supplier a
           WHERE nvl(b.depositremain, 0) <>
                 nvl(a.deposit_pay, 0) - nvl(a.deposit_payable, 0)
           AND a.c_supplier_id = b.id)
    AND b.id = :NEW.c_supplier_id;
    --end by xkk  20160415

END;/* edit by lauren 2017/5/15 11:16:04 */ ;
/

