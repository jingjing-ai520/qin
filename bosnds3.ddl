CREATE OR REPLACE TRIGGER BOSNDS3.BU_USERS 
    BEFORE UPDATE ON BOSNDS3.USERS 
    FOR EACH ROW 
declare
    -- local variables here
    /* eiffie 20101101
    建立AFTER行级触发器BU_USERS,程序逻辑:
    如果修改用户前[可用]的值为N且修改后[可用]的值为Y,
    则后台自动使该用户在时间=(当前日期 -参数portal.4041当前值+3)登录过一次,
    即新增表[C_SYSLOG]记录, 模块['SEC']、子模块['login']、级别[3]、操作者[该用户的名称]、客户端['0.0.0.0']、内容['该用户长时间未登入,手动设置成可用状态并允许用户在3天内登入。']、
    创建时间[当前日期-参数portal.4041当前值+3的时间]。*/
    v_para number(10);
    v_old  varchar2(2);
    v_new  varchar2(2);
begin
    select value into v_para from AD_PARAM where name = 'portal.4041';

    if (:OLD.isactive = 'N' AND :NEW.isactive = 'Y') then
        insert into C_SYSLOG
            (ID, MODULE, SUBMODULE, PRIORITY, OPERATOR, REMOTE_HOST, MESSAGE,
             CREATIONDATE, AD_CLIENT_ID)
        values
            (get_sequences('C_SYSLOG'), 'SEC', 'login', '3', :new.name,
             '0.0.0.0', '该用户长时间未登入,手动设置成可用状态并允许用户在3天内登入。', sysdate - v_para + 3,
             37);
       -- update_user(:new.id);
        --update users a set a.passwordreset = 'Y' where a.truename = :new.name;
    end if;
end BU_USERS; 
/

