    FOR EACH ROW 
declare
    --将审批意见写到comments里
    v_table_id number(10);
begin
    if :old.state = 'W' and :new.state <> 'W' then
        -- comments tablename should the realtable name, not view name
        select nvl(t.realtable_id, t.id)
        into v_table_id
        from ad_table t, au_phaseinstance pi
        where pi.id = :new.au_pi_id and t.id = pi.ad_table_id;

        insert into u_comments
            (id, ad_client_id, ad_org_id, isactive, modifieddate, creationdate,
             ownerid, modifierid, TABLENAME, record_id, CONTENT)
            select get_sequences('u_comments'), u.ad_client_id, u.ad_org_id, 'Y',
                   sysdate, sysdate, u.id, u.id, t.name, pi.record_id,
                   decode(:new.state, 'A', '(批准)', 'R', '(驳回)', '') ||
                    :new.comments
            from users u, au_phaseinstance pi, ad_table t
            where u.id = :new.ad_user_id and pi.id = :new.au_pi_id and
                  t.id = v_table_id;
    end if;
end; 
/

