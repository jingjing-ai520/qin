CREATE OR REPLACE TRIGGER BOSNDS3.BI_C_CLIENT_VIP 
    AFTER UPDATE ON BOSNDS3.C_CLIENT_VIP 
    FOR EACH ROW 
BEGIN
    IF UPDATING THEN
        /*
           [VIP卡号]修改前和修改后的值不同、或者[VIP类型]修改前和修改后的值不同则写入表[C_CLIENT_VIP_LOG]。
          注意:表[VIP修改日志]的字段[修改人、修改时间]取[VIP档案]的[修改人、修改时间]。
        */
        IF (:NEW.CARDNO <> :OLD.CARDNO) OR
           (:OLD.C_VIPTYPE_ID <> :NEW.C_VIPTYPE_ID) THEN
        
            INSERT INTO c_client_vip_log
                (ID, AD_CLIENT_ID, AD_ORG_ID, OLD_CARDNO, NEW_CARDNO,
                 C_OLDVIPTYPE_ID, C_NEWVIPTYPE_ID, C_VIP_ID, OWNERID, MODIFIERID,
                 CREATIONDATE, MODIFIEDDATE, ISACTIVE)
            VALUES
                (get_sequences('c_client_vip_log'), :NEW.ad_client_id,
                 :NEW.ad_org_id, :OLD.cardno, :NEW.cardno, :OLD.C_VIPTYPE_ID,
                 :NEW.C_VIPTYPE_ID, :NEW.ID, :NEW.ownerid,
                 :NEW.modifierid, SYSDATE, :NEW.MODIFIEDDATE, :NEW.isactive);
        END IF;
    END IF;

END BI_C_CLIENT_VIP; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.BI_FA_STORAGE 
    AFTER INSERT OR UPDATE OF QTYPREOUT, QTY ON BOSNDS3.FA_STORAGE 
    FOR EACH ROW 
BEGIN
    MERGE INTO jk_storage g
    USING (SELECT a.c_store_id, nvl(:NEW.qty - :NEW.qtypreout, 0) AS qty
           FROM wm_storage a
           WHERE a.isactive = 'Y'
           AND a.c_store_id = :NEW.c_store_id) u
    ON (g.id = :NEW.id)
    WHEN MATCHED THEN
        UPDATE
        SET g.modifieddate = SYSDATE, g.status = 1, g.qty = u.qty
        WHERE nvl(g.qty, 0) <> u.qty
    WHEN NOT MATCHED THEN
        INSERT
            (id, ad_client_id, modifieddate, status, qty, m_product_id,
             m_productalias_id, c_store_id)
        VALUES
            (:NEW.id, 37, SYSDATE, 1, u.qty, :NEW.m_product_id,
             :NEW.m_productalias_id, :NEW.c_store_id);
END; 
/

