CREATE OR REPLACE TRIGGER BOSNDS3.TI_BD_C_SUPPLIER 
    BEFORE DELETE ON BOSNDS3.C_SUPPLIER 
    FOR EACH ROW 
DECLARE
    PRAGMA AUTONOMOUS_TRANSACTION;
    v_count NUMBER(10); --记录条数
    p_id    NUMBER(10); --数值
BEGIN
    --获取该供应商ID
    p_id := :old.id;

    --面料/辅料采购单中已引用此供应商，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM y_purchase
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';
    --增加控制：面料/辅料采购单中已引用此供应商，不能删除
    IF v_count > 0 THEN
        raise_application_error('-20201', '面料/辅料采购单中已引用此供应商，不能删除！');
    END IF;

    --获取物料直发单的记录条数
    SELECT COUNT(1)
    INTO v_count
    FROM y_dir_pur
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';

    --增加控制：面料/辅料直发单中已引用此供应商，不能删除
    IF v_count > 0 THEN
        raise_application_error('-20201', '面料/辅料直发单中已引用此供应商，不能删除！');
    END IF;

    --获取对应供应商报价单的记录条数
    SELECT COUNT(1)
    INTO v_count
    FROM y_purchasebid
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';
    --增加控制：供应商报价单中已引用此供应商，不能删除
    IF v_count > 0 THEN
        raise_application_error('-20201', '供应商报价单中已引用此供应商，不能删除！');
    END IF;

    --获取对应生产通知单的记录条数
    SELECT COUNT(1)
    INTO v_count
    FROM y_inform
    WHERE (y_supplier_fab_id = p_id OR y_supplier_acc_id = p_id)
    AND isactive = 'Y';
    --增加控制：生产通知单中已引用此供应商，不能删除
    IF v_count > 0 THEN
        raise_application_error('-20201', '生产通知单中已引用此供应商，不能删除！');
    END IF;

    --面料/辅料资料定义中已引用此供应商，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM y_material
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count > 0 THEN
        raise_application_error('-20201', '面料/辅料资料定义中已引用此供应商，不能删除！');
    END IF;

    --物料采购订单中已引用此供应商，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM y_plan
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count > 0 THEN
        raise_application_error('-20201', '物料采购订单中已引用此供应商，不能删除！');
    END IF;

    --物料采购退货单中已引用此供应商，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM y_purret
    WHERE y_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count > 0 THEN
        raise_application_error('-20201', '物料采购退货单中已引用此供应商，不能删除！');
    END IF;

    --发生过采购单或者采购入库单，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM m_purchase
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '采购单或者采购入库单已引用此供应商，不能删除！');
    END IF;

    --发生过采购订单，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM b_po
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '采购订单已引用此供应商，不能删除！');
    END IF;

    --发生过采购退货单，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM m_ret_pur
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '采购退货单已引用此供应商，不能删除！');
    END IF;

    --发生过供应商应付款，不能删除
    SELECT COUNT(1)
    INTO v_count
    FROM b_payable
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '供应商账户应付单已引用此供应商，不能删除！');
    END IF;

    --发生过供应商付款，不能删
    SELECT COUNT(1)
    INTO v_count
    FROM b_pay
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '供应商账户付款单已引用此供应商，不能删除！');
    END IF;

    --采购订单批量调整单
    SELECT COUNT(1)
    INTO v_count
    FROM b_pobadj
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '采购订单批量调整单已引用此供应商，不能删除！');
    END IF;

    --直发单
    SELECT COUNT(1)
    INTO v_count
    FROM m_direct
    WHERE c_supplier_id = p_id
    AND isactive = 'Y';

    IF v_count <> 0 THEN
        raise_application_error(-20201, '直发单已引用此供应商，不能删除！');
    END IF;
END; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.TI_FA_STORAGE_TEUS_FTP 
    BEFORE INSERT ON BOSNDS3.FA_STORAGE_TEUS_FTP 
    FOR EACH ROW 
DECLARE

    v_storage_teus_id fa_storage_teus.id%TYPE;

BEGIN
    --判断该店仓下是否存在该箱的库存
    SELECT MAX(id)
    INTO v_storage_teus_id
    FROM fa_storage_teus
    WHERE ad_client_id = :NEW.ad_client_id
    AND m_teus_id = :NEW.m_teus_id
    AND c_store_id = :NEW.c_store_id;

    --如果不存在则新增一条记录
    IF v_storage_teus_id IS NULL THEN
        INSERT INTO fa_storage_teus
            (id, ad_client_id, ad_org_id, ownerid, modifierid, creationdate,
             modifieddate, isactive, c_store_id, m_teus_id, qty)
        VALUES
            (get_sequences('FA_STORAGE'), :NEW.ad_client_id, :NEW.ad_org_id,
             :NEW.ownerid, :NEW.ownerid, SYSDATE, SYSDATE, 'Y', :NEW.c_store_id,
             :NEW.m_teus_id, :NEW.qtychange);
    ELSE
        --如果存在则更新数量
        UPDATE fa_storage_teus
        SET qty = nvl(qty, 0) + :NEW.qtychange
        WHERE id = v_storage_teus_id;
    END IF;
END; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.TIFACUSTOMERBLOCKFTP 
    BEFORE INSERT ON BOSNDS3.FA_CUSTOMERBLOCK_FTP 
    FOR EACH ROW 
DECLARE
    ---- Author: GLY
    -- Date: 2017-05-15
    -- Description:
    --实现多资金账户FA_CUSTOMERBLOCK的应收金额、已收金额的累加（根据经销商+资金账户）。
    v_id NUMBER(10);

BEGIN
    SELECT MAX(id)
    INTO v_id
    FROM fa_customerblock
    WHERE ad_client_id = :new.ad_client_id
    AND c_customer_id = :new.c_customer_id
    AND c_account_id = :new.c_account_id
    AND rownum < 2;

    IF v_id IS NULL THEN

        -- 如果没有，就新增

        INSERT INTO fa_customerblock
            (id, ad_client_id, ad_org_id, ownerid, modifierid, creationdate,
             modifieddate, isactive, c_customer_id, amt_receivable, amt_receive,
             amt_odr_receivable, amt_odr_receive, c_account_id)
        VALUES
            (get_sequences('fa_customerblock'), :new.ad_client_id,
             :new.ad_org_id, :new.ownerid, :new.ownerid, SYSDATE, SYSDATE, 'Y',
             :new.c_customer_id, :new.amt_receivable, :new.amt_receive,
             :new.amt_odr_receivable, :new.amt_odr_receive, :new.c_account_id);

    ELSE

        --实现多资金账户FA_CUSTOMERBLOCK的应收金额、已收金额的累加（根据经销商+资金账户）。
        UPDATE fa_customerblock
        SET amt_receivable = amt_receivable + :new.amt_receivable,
            amt_receive = amt_receive + :new.amt_receive,
            amt_odr_receivable = amt_odr_receivable + :new.amt_odr_receivable,
            amt_odr_receive = amt_odr_receive + :new.amt_odr_receive
        WHERE id = v_id;

    END IF;

END;/* edit by 王俊可 2017/5/18 10:03:45 */ ;
/

CREATE OR REPLACE TRIGGER BOSNDS3.TIFACUSTOMERFTP 
    BEFORE INSERT ON BOSNDS3.FA_CUSTOMER_FTP 
    FOR EACH ROW 
declare

    v_ID       number(10);



    /**
    * qtychange需要根据实际的业务单据来置入，当增加库存的业务发生后是正数，减少库存的业务发生后是负数
    *docno是单据编号，仅记录
    *负库存控制

    */

begin
    -- get current 资金余额


    select MAX(ID)
    into v_ID
    from FA_CUSTOMER
    where
          ad_client_id=:new.ad_client_id and
          C_CUSTOMER_ID = :new.C_CUSTOMER_ID  AND ROWNUM < 2;

    if v_ID IS NULL then


        -- insert FA_CUSTOMER

        INSERT INTO FA_CUSTOMER
            (ID,AD_CLIENT_ID,AD_ORG_ID,OWNERID,MODIFIERID,CREATIONDATE,MODIFIEDDATE,ISACTIVE,
            C_CUSTOMER_ID,AMT_RECEIVABLE,AMT_RECEIVE,
            AMT_ODR_RECEIVAblE, AMT_ODR_RECEIVE)
        VALUES
            (GET_SEQUENCES('FA_CUSTOMER'), :NEW.AD_CLIENT_ID, :NEW.AD_ORG_ID,:NEW.OWNERID,:NEW.OWNERID,sysdate, sysdate,'Y',
             :NEW.C_CUSTOMER_ID,:NEW.AMT_RECEIVABLE, :NEW.AMT_RECEIVE,
             :NEW.AMT_ODR_RECEIVAblE, :NEW.AMT_ODR_RECEIVE);

    else

