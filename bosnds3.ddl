CREATE OR REPLACE TRIGGER BOSNDS3.BI_M_SALE 
    BEFORE UPDATE OF C_STORE_ID, ISACTIVE OR DELETE ON BOSNDS3.M_SALE 
    FOR EACH ROW 
BEGIN
    IF updating AND :NEW.isactive = 'N' AND :OLD.hlock_status = 2 THEN
        raise_application_error(-20201, '单据已锁定，不能作废！');
    END IF;

    IF updating AND :NEW.c_store_id <> :OLD.c_store_id AND
       :OLD.hlock_status = 2 THEN
        raise_application_error(-20201, '单据已锁定，不能变更发货店仓！');
    END IF;

    IF deleting AND :OLD.hlock_status = 2 THEN
        raise_application_error(-20201, '单据已锁定，不能删除！');
    END IF;

END; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.BI_M_SALEITEM 
    BEFORE INSERT OR UPDATE OF QTY OR DELETE ON BOSNDS3.M_SALEITEM 
    FOR EACH ROW 
DECLARE
    v_count NUMBER;
BEGIN

    SELECT MAX(a.hlock_status)
    INTO v_count
    FROM m_sale a
    WHERE a.id = nvl(:OLD.m_sale_id, :NEW.m_sale_id)
    AND a.status = 1;

    IF v_count = 2 THEN
    
        IF (inserting OR deleting) THEN
            raise_application_error(-20201, '单据已锁定，不能新增或删除明细！');
        END IF;
    
        IF updating AND nvl(:NEW.qty, 0) <> nvl(:OLD.qty, 0) THEN
            raise_application_error(-20201, '单据已锁定，不能修改明细！');
        END IF;
    
    END IF;

END; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.BI_UPWEBPOSTIME 
    BEFORE INSERT OR UPDATE ON BOSNDS3.WEBPOS_LOG 
    FOR EACH ROW 
DECLARE

    v_webpos_log_id NUMBER(10);
    v_up_times      webpos_log.up_times%TYPE;
    v_c_store_id    webpos_log.c_store_id%TYPE;
    v_creationdate  webpos_log.creationdate%TYPE;
    v_ownerid       webpos_log.ownerid%TYPE;
    v_isclear       webpos_log.iscreclear%TYPE;
    v_iscreclear    webpos_log.isclear%TYPE;
    v_plan_num      webpos_log.plan_num%TYPE; --add by csy 20121210 机器号
BEGIN
    IF inserting THEN

        --begin modified by csy 20121210 增加机器号的取值
        BEGIN
            SELECT h.id, h.up_times, h.c_store_id, h.creationdate, h.ownerid,
                   h.iscreclear, h.isclear, h.plan_num
            INTO v_webpos_log_id, v_up_times, v_c_store_id, v_creationdate,
                 v_ownerid, v_iscreclear, v_isclear, v_plan_num
            FROM (SELECT a.id, a.up_times, a.c_store_id, a.creationdate,
                          a.ownerid, a.iscreclear, a.isclear, a.plan_num
                   FROM webpos_log a
                   WHERE a.c_store_id = :NEW.c_store_id
                   AND to_char(a.creationdate, 'YYYYMMDD') =
                         to_char(:NEW.creationdate, 'YYYYMMDD')
                   AND a.plan_num = :NEW.plan_num
                   ORDER BY a.up_times DESC) h
            WHERE rownum = 1;
        EXCEPTION
            WHEN OTHERS THEN
                v_up_times := 0;
        END;
        --end by csy 20121210

        IF nvl(v_ownerid, -1) = :NEW.ownerid THEN
            :NEW.creationdate := v_creationdate;
            :NEW.up_times := v_up_times;
            :NEW.iscreclear := v_iscreclear;
            :NEW.isclear := v_isclear;
            DELETE FROM webpos_log t
            WHERE t.id = v_webpos_log_id;
        ELSE
            :NEW.up_times := nvl(v_up_times, 0) + 1;
        END IF;
    END IF;

END bi_upwebpostime; 
/

