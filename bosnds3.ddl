CREATE OR REPLACE TRIGGER BOSNDS3.AU_C_STORE 
    AFTER UPDATE ON BOSNDS3.C_STORE 
    FOR EACH ROW 
DECLARE
BEGIN

    IF :OLD.is_allo2ostorage = 'Y' AND :NEW.is_allo2ostorage = 'N' THEN
        UPDATE o2o_storagerate t
        SET t.is_open = 'N', t.modifieddate = SYSDATE
        WHERE t.c_store_id = :NEW.id;

        --add by wangping 20170707 更新线上库存比例设置中的启用值
        UPDATE o2o_storate_online t
        SET t.is_open = 'N', t.modifieddate = SYSDATE
        WHERE t.c_store_id = :NEW.id;
        --end by wangping 20170707

    ELSIF :OLD.is_allo2ostorage = 'N' AND :NEW.is_allo2ostorage = 'Y' THEN
        UPDATE o2o_storagerate t
        SET t.is_open = 'Y', t.modifieddate = SYSDATE
        WHERE t.c_store_id = :NEW.id;

        --add by wangping 20170707 更新线上库存比例设置中的启用值
        UPDATE o2o_storate_online t
        SET t.is_open = 'Y', t.modifieddate = SYSDATE
        WHERE t.c_store_id = :NEW.id;
        --end by wangping 20170707
    END IF;

