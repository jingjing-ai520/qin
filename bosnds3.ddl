CREATE OR REPLACE TRIGGER BOSNDS3.TDMPRODUCTALIAS 
    BEFORE DELETE ON BOSNDS3.M_PRODUCT_ALIAS 
    FOR EACH ROW 
DECLARE

    PRAGMA AUTONOMOUS_TRANSACTION;
    v_count NUMBER(10);

BEGIN

    SELECT COUNT(1)
    INTO v_count
    FROM b_poitem
    WHERE m_product_id = :OLD.m_product_id
    AND m_attributesetinstance_id = :OLD.m_attributesetinstance_id;

    IF v_count <> 0 THEN
        raise_application_error(-20201, '该条码已做过采购订单，不能删除');
    END IF;

    SELECT COUNT(1)
    INTO v_count
    FROM m_purchaseitem
    WHERE m_product_id = :OLD.m_product_id
    AND m_attributesetinstance_id = :OLD.m_attributesetinstance_id;

    IF v_count <> 0 THEN
        raise_application_error(-20201, '该条码已做过采购单，不能删除');
    END IF;

    SELECT COUNT(1)
    INTO v_count
    FROM m_ret_puritem
    WHERE m_product_id = :OLD.m_product_id
    AND m_attributesetinstance_id = :OLD.m_attributesetinstance_id;
    IF v_count <> 0 THEN
        raise_application_error(-20201, '该条码已做过采购退货单，不能删除');
    END IF;

    SELECT COUNT(1)
    INTO v_count
    FROM b_soitem
    WHERE m_product_id = :OLD.m_product_id
    AND m_attributesetinstance_id = :OLD.m_attributesetinstance_id;
    IF v_count <> 0 THEN
        raise_application_error(-20201,
                                '该条码已做过发货订单，不能删除' || :OLD.m_product_id || ' ' ||
                                 :OLD.m_attributesetinstance_id);
    END IF;

    SELECT COUNT(1)
    INTO v_count
    FROM m_saleitem
    WHERE m_product_id = :OLD.m_product_id
    AND m_attributesetinstance_id = :OLD.m_attributesetinstance_id;
    IF v_count <> 0 THEN
        raise_application_error(-20201, '该条码已做过发货单，不能删除');
    END IF;

    SELECT COUNT(1)
    INTO v_count
    FROM m_ret_saleitem
