CREATE OR REPLACE TRIGGER BOSNDS3.TRI_C_SYSLOG_BI 
    BEFORE INSERT ON BOSNDS3.C_SYSLOG 
    FOR EACH ROW 
declare
    /**
    *  2005-12-27
    *  程序处理ad_client_id, 如果为-1 的话
    */
    v_client_id number(10);
begin
    if :new.ad_client_id = -1 then
        select max(id)
        into v_client_id
        from ad_client
        where domain = substr(:new.operator, instr(:new.operator, '@') + 1);
        if v_client_id is not null then
            :new.ad_client_id := v_client_id;
            :new.operator     := substr(:new.operator,
                                        1,
                                        instr(:new.operator, '@') - 1);
        end if;
    end if;
end; 
/

