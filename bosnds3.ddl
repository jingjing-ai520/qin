CREATE OR REPLACE TRIGGER BOSNDS3.TUUSERS 
    BEFORE UPDATE ON BOSNDS3.USERS 
    FOR EACH ROW 
declare
    /*判断comm@next99.cn是否修改了密码*/
    /**
          yfzhu 2010-5-6 更新email地址的时候，连带更新lportal相关表的内容
          yfzhu 2010-8-18 发现contact 设置where 条件中存在错误，使用了v_nuserid
    */
    v_ouserid VARCHAR2(80);
    v_nuserid varchar2(80);

begin
    if (:old.email = 'comm@next99.cn' or :old.email = 'test2@next99.cn' or
       :old.email = 'guest@next99.cn') and
       :new.passwordhash <> :old.passwordhash then
        rollback;
    end if;
    if :old.email <> :new.email then
        SELECT :old.email || '@' || c.domain, :new.email || '@' || c.domain
        INTO v_ouserid, v_nuserid
        FROM ad_client c
        WHERE :old.ad_client_id = c.id;

        update user_@lportal
        set userid = v_nuserid
        where userid = v_ouserid;

        update contact_@lportal
        set userid = v_nuserid, contactid = v_nuserid
        where userid = v_ouserid and contactid = v_ouserid;

        update group_@lportal
        set classpk = v_nuserid
        where classpk = v_ouserid;

        update users_roles@lportal
        set userid = v_nuserid
        where userid = v_ouserid;

        update users_usergroups@lportal
        set userid = v_nuserid
        where userid = v_ouserid;
    end if;
end; 
/



