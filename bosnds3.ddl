CREATE OR REPLACE TRIGGER BOSNDS3.TIYFASTOREGEFTP 
    BEFORE INSERT ON BOSNDS3.Y_FA_STORAGE_FTP 
    FOR EACH ROW 
DECLARE
    v_id         NUMBER(10);
    v_qty        NUMBER(18);
    v_pdt_name   y_material.NAME%TYPE;
    v_isnegative y_warehouse.isnegative%TYPE; --add by csy 20130108负库存
    v_3003param  VARCHAR2(20); --参数3003的值。
BEGIN
    -- get current storage
    SELECT NAME
    INTO v_pdt_name
    FROM y_material
    WHERE id = :NEW.y_material_id;

    --begin add by lily 2013-03-22 获取系统参数3003的值。
    SELECT upper(VALUE)
    INTO v_3003param
    FROM ad_param a
    WHERE NAME = 'product.3003';

    --end by lily 2013-03-22

    SELECT MAX(id)
    INTO v_id
    FROM y_fa_storage
    WHERE ad_client_id = :NEW.ad_client_id
    AND y_warehouse_id = :NEW.y_warehouse_id
    AND y_materialalias_id = :NEW.y_materialalias_id
    AND ((nvl(m_product_id, 0) = nvl(:NEW.m_product_id, 0) AND
          v_3003param = 'TRUE') OR (v_3003param = 'FALSE'))
    AND rownum < 2;

    --add by csy 20130108 查询仓库是否负库存
    SELECT isnegative
    INTO v_isnegative
    FROM y_warehouse
    WHERE id = :NEW.y_warehouse_id;
    -- end by csy

    IF v_id IS NULL THEN

        IF :NEW.qtychange < 0 THEN

            --add by csy  调整后的实际库存不能为负数
            IF v_isnegative = 'N' THEN
                raise_application_error(-20201, '调整后的实际库存不能为负数:' || v_pdt_name);
            END IF;
            --end by csy
        END IF;

        -- insert Y_FA_STORAGE

        --modified by csy 20121017 增加条码的插入
        --begin modified by vetica 20130523 不再向库存表中写入幅宽、单位、颜色、规格等信息，保证库存表仓库+物料+款号的唯一性
        INSERT INTO y_fa_storage
            (id, ad_client_id, ad_org_id, ownerid, modifierid, creationdate,
             modifieddate, isactive, y_warehouse_id, y_material_id, m_product_id,
             qty, y_materialalias_id)
        VALUES
            (get_sequences('Y_FA_STORAGE'), :NEW.ad_client_id, :NEW.ad_org_id,
             :NEW.ownerid, :NEW.ownerid, SYSDATE, SYSDATE, 'Y',
             :NEW.y_warehouse_id, :NEW.y_material_id,
             CASE WHEN v_3003param = 'TRUE' THEN :NEW.m_product_id ELSE NULL END,
             :NEW.qtychange, :NEW.y_materialalias_id);
        --end modified by vetica 20130523
        --end by csy

    ELSE
        SELECT qty + :NEW.qtychange
        INTO v_qty
        FROM y_fa_storage
        WHERE id = v_id;

        IF v_qty < 0 AND :NEW.qtychange < 0 THEN
            --add by csy  调整后的实际库存不能为负数
            IF v_isnegative = 'N' THEN
                raise_application_error(-20201, '调整后的实际库存不能为负数:' || v_pdt_name);
            END IF;
            --end by csy
        END IF;
        -- END IF;

        --update
        UPDATE y_fa_storage
        SET qty = nvl(qty, 0) + :NEW.qtychange
        WHERE id = v_id;

    END IF;

END; 
/

CREATE OR REPLACE TRIGGER BOSNDS3.TRI_AU_PHASE_BUD 
    BEFORE INSERT OR UPDATE OR DELETE ON BOSNDS3.AU_PHASE 
    FOR EACH ROW 
DECLARE
    v_cnt NUMBER(10);
BEGIN

    SELECT COUNT(*)
    INTO v_cnt
    FROM au_phaseinstance pi
    WHERE pi.au_process_id = :new.au_process_id
    AND rownum < 2;
    --modify by hzy 20150114 修改控制逻辑
    --原逻辑
    /*  IF v_cnt > 0 THEN
            raise_application_error(-20201, '已有工作流实例, 请设定新的流程实现变更(版本化)');
        END IF;
    */
    --现逻辑：如果有了工作流实例、且除了3个字段(最少审批人数、最少批准人数、审核用户)，
    --修改界面可编辑字段(比如：名称、序号)里如果有任意一个字段的new值<>old值，则不允许,
    --提示：已有工作流实例, 请设定新的流程实现变更(版本化)。
    IF v_cnt > 0 AND
       (nvl(:new.name, -11) <> nvl(:old.name, -11) OR
       nvl(:new.orderno, -11) <> nvl(:old.orderno, -11) OR
       nvl(:new.auto_assign, -11) <> nvl(:old.auto_assign, -11) OR
       nvl(:new.description, -11) <> nvl(:old.description, -11) OR
       :new.filterobj <> :old.filterobj OR
       (:new.filterobj IS NULL AND :old.filterobj IS NOT NULL) OR
       (:new.filterobj IS NOT NULL AND :old.filterobj IS NULL) OR
       nvl(:new.succ_action, -11) <> nvl(:old.succ_action, -11) OR
       nvl(:new.succ_program, -11) <> nvl(:old.succ_program, -11) OR
       nvl(:new.fail_action, -11) <> nvl(:old.fail_action, -11) OR
       nvl(:new.fail_program, -11) <> nvl(:old.fail_program, -11) OR
       nvl(:new.waittime, -11) <> nvl(:old.waittime, -11) OR
       nvl(:new.timeout_action, -11) <> nvl(:old.timeout_action, -11) OR
       nvl(:new.timeout_program, -11) <> nvl(:old.timeout_program, -11) OR
       nvl(:new.ad_column1_id, -11) <> nvl(:old.ad_column1_id, -11) OR
       nvl(:new.ad_column2_id, -11) <> nvl(:old.ad_column2_id, -11) OR
       nvl(:new.ad_column3_id, -11) <> nvl(:old.ad_column3_id, -11) OR
       nvl(:new.isactive, -11) <> nvl(:old.isactive, -11)) THEN

        raise_application_error(-20201, '已有工作流实例, 请设定新的流程实现变更(版本化)');
    END IF;
    --end 20150114
END;/* edit by 薛飞 2015/1/21 11:22:34 */ ;
/

